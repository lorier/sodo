extends ../_bootstrap
append styles
	// Custom styles for this template
	//- link(href='../css/starter-template.css', rel='stylesheet')
block prepend body
	body.property
	+navbar-fixed("Sodo Urbanworks","navbar","inverse")
		+nav_item("property.html","active") The Property
		+nav_item("available-spaces.html") Available Spaces
		+nav_item("tenant-directory.html") Tenant Directory
		+nav_item("leasing-info.html") Leasing Info
	.jumbotron
		.container
			.row
				.col-md-12
	.container.container-intro.voffset-lrg 
		.intro-wrap
			.row
				.col-md-6
					.intro
						.double-rule-cyan
						h1 The Property
						p SoDo Urbanworks (Formerly City Commerce Business Park) is a multi-tenant property located in the heart of South Seattle’s thriving SoDo neighborhood. Built in 1946, the business park sits on 6.8 acres that includes customer parking and greenspace. Our tenants appreciate the easy access, well-maintained public spaces, and eclectic mix of businesses.
						ul.stats
							li 
								strong Total Rentable Space
								|: 27,882 SF
							li 
								strong Rental Rate
								|: $0.90 - $1.33 /SF/Month
							li 
								strong Min. Divisible Space
								|: 4,406 SF
							li 
								strong Max. Contiguous Space
								|: 6,065 SF
							li 
								strong Zoning Designation
								|: Industrial General (IG1 U/85)
				.col-md-6.benefits
					div
						h3 Benefits
						ul.list-unstyled
							li 
								+icon("ok")
								|  Fast Growing Neighborhood
							li 
								+icon("ok")
								|  Proximity to downtown and transit
							li 
								+icon("ok")
								|  Easy access to I-5
							li 
								+icon("ok")
								|  Directly visibility from 1st Ave S
							li 
								+icon("ok")
								|  Dock and grade-level loading  
							li 
								+icon("ok")
								|  16’ to 20’ clear height  
						a.btn.btn-lg.btn-white(href='#', role='button') 
							+icon("download-alt")
							&nbsp;
							| Download Sales Sheet
		.row.site-plan
			.col-md-12
				div
		.row.voffset-lrg.hidden-xs.hidden-sm
			.col-md-12.double-rule-gray
				div
		.row.voffset-lrg
			.col-md-12.big-map
				div
					a.btn.btn-lg.btn-white.panel-button(href='available-spaces.html', role='button') View Map &gt;
	.tenants-wrapper
		.container.tenants.voffset-lrg
			.row
				.col-md-12
					span.double-rule
					h2.our-tenants Our Tenants
					span.double-rule
			.row.tenants-row
				.col-lg-2.col-md-3.col-sm-6.logo1
					div
						img(src="../tw-images/vespa-logo.png")
				.col-lg-2.col-md-3.col-sm-6.logo2
					div
						img(src="../tw-images/vespa-logo.png")
				.col-lg-2.col-md-3.col-sm-6.logo3
					div
						img(src="../tw-images/vespa-logo.png")
				.col-lg-2.col-md-3.col-sm-6.logo4
					div
						img(src="../tw-images/vespa-logo.png")
				.col-lg-2.col-md-3.col-sm-6.logo5
					div
						img(src="../tw-images/vespa-logo.png")
				.col-lg-2.col-md-3.col-sm-6.logo6
					div
						img(src="../tw-images/vespa-logo.png")
			.row
				.col-md-12.button-container
					a.btn.btn-lg.btn-white(href='#', role='button') See All Tenants &gt;
	include _footer.jade
